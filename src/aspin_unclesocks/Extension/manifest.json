{
    "manifest_version": 3,
    "name": "Aspin: Filipino Passphrase Generator",
    "version": "2025.1.0",
    "description": "A feature-rich passphrase generator for Filipino and English.",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "aspin.html",
        "default_icon": {
            "16": "assets/icon.png",
            "48": "assets/icon.png",
            "128": "assets/icon.png"
        }
    },
    "icons": {
        "16": "assets/icon.png",
        "48": "assets/icon.png",
        "128": "assets/icon.png"
    }
}
